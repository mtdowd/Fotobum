
<%= form_for [gallery, image], data: {confirm: "Are you sure?"} do |form| %>
  <div>
    <%= form.label :name, "Image Name: " %>
    <%= form.text_field :name %>
  </div>
  <br>
  <div>
    <%= form.label :description, "Image Description: " %>
    <%= form.text_area :description %>
  </div>
  <br>
  <div>
    <%= form.label :url, "URL: " %>
    <%= form.text_field :url %>
  </div>
  <br>
  <div>
    <%= form.submit %>
  </div>
<% end %>
